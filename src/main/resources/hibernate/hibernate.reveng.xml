<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>


	<table name="profil" catalog="store">
		 <set property="users"
   					exclude="true" ></set>
	</table>

	<table-filter match-catalog="store" match-name="profil" />
	<table-filter match-catalog="store" match-name="user" />
	<table-filter match-name="acl_store" match-catalog="store"></table-filter>
	<table-filter match-name="droit" match-catalog="store"></table-filter>
	<table-filter match-name="ressource" match-catalog="store"></table-filter>
</hibernate-reverse-engineering>